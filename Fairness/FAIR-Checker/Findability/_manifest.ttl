@prefix mf:      <http://www.w3.org/2001/sw/DataAccess/tests/test-manifest#> .
@prefix kgi: <http://ns.inria.fr/kg/index#> .
@prefix dcterms: <http://purl.org/dc/terms/> .

<> a mf:Manifest ;
    mf:entries (
        <F1A.ttl>
        <F1B.ttl>
        <F2A.ttl>
    ) .

<F1A.ttl> a mf:ManifestEntry ;
    kgi:onSuccess (
        [
            dcterms:description "There is no dataset description" ;
            mf:action """
                PREFIX void: <http://rdfs.org/ns/void#>
                PREFIX dcat: <http://www.w3.org/ns/dcat#>
                PREFIX dcmitype: <http://purl.org/dc/dcmitype/>
                PREFIX schema: <http://schema.org/>
                PREFIX sd: <http://www.w3.org/ns/sparql-service-description#>
                PREFIX prov: <http://www.w3.org/ns/prov#>
                PREFIX pav: <http://purl.org/pav/>
                PREFIX foaf: <http://xmlns.com/foaf/0.1/>
                PREFIX dqv: <http://www.w3.org/ns/dqv#>
                PREFIX dce: <http://purl.org/dc/elements/1.1/>
                PREFIX dcterms: <http://purl.org/dc/terms/>
                PREFIX dataid: <http://dataid.dbpedia.org/ns/core#>
                INSERT {
                    ?kgGraph dqv:hasQualityMeasurement [
                        a dqv:QualityMeasurement ;
                        rdfs:label "Unique IDs"@en ;
                        dqv:isMeasurementOf <https://w3id.org/fair/principles/latest/F1#F1A> ;
                        dqv:value 0
                    ] .
                } WHERE {
                    FILTER(NOT EXISTS {
                        GRAPH ?kgGraph {
                            ?kg a dcat:Dataset .
                        }
                    })
                    BIND(Iri(CONCAT("http://ns.inria.fr/kg/index#", MD5(str($rawEndpointUrl)), "Graph")) AS ?kgGraph)
                }
                """
        ]
        [
            dcterms:description "There is a dataset description. The dataset description contains no blank nodes, meaning every identifiers should be unique." ;
            mf:action """
                PREFIX void: <http://rdfs.org/ns/void#>
                PREFIX dcat: <http://www.w3.org/ns/dcat#>
                PREFIX dcmitype: <http://purl.org/dc/dcmitype/>
                PREFIX schema: <http://schema.org/>
                PREFIX sd: <http://www.w3.org/ns/sparql-service-description#>
                PREFIX prov: <http://www.w3.org/ns/prov#>
                PREFIX pav: <http://purl.org/pav/>
                PREFIX foaf: <http://xmlns.com/foaf/0.1/>
                PREFIX dqv: <http://www.w3.org/ns/dqv#>
                PREFIX dce: <http://purl.org/dc/elements/1.1/>
                PREFIX dcterms: <http://purl.org/dc/terms/>
                PREFIX dataid: <http://dataid.dbpedia.org/ns/core#>
                INSERT {
                    ?kgGraph dqv:hasQualityMeasurement [
                        a dqv:QualityMeasurement ;
                        rdfs:label "Unique IDs"@en ;
                        dqv:isMeasurementOf <https://w3id.org/fair/principles/latest/F1#F1A> ;
                        dqv:value 2
                    ] .
                } WHERE {
                    FILTER(NOT EXISTS {
                        GRAPH ?kgGraph {
                            ?kg a dcat:Dataset .
                            ?s ?p ?o .
                        }
                        FILTER(isBlank(?s) || isBlank(?p) || isBlank(?o))
                    })
                }
                """
        ]
        [
            dcterms:description "There is a dataset description. The dataset description contains some blank nodes." ;
            mf:action """
                PREFIX void: <http://rdfs.org/ns/void#>
                PREFIX dcat: <http://www.w3.org/ns/dcat#>
                PREFIX dcmitype: <http://purl.org/dc/dcmitype/>
                PREFIX schema: <http://schema.org/>
                PREFIX sd: <http://www.w3.org/ns/sparql-service-description#>
                PREFIX prov: <http://www.w3.org/ns/prov#>
                PREFIX pav: <http://purl.org/pav/>
                PREFIX foaf: <http://xmlns.com/foaf/0.1/>
                PREFIX dqv: <http://www.w3.org/ns/dqv#>
                PREFIX dce: <http://purl.org/dc/elements/1.1/>
                PREFIX dcterms: <http://purl.org/dc/terms/>
                PREFIX dataid: <http://dataid.dbpedia.org/ns/core#>
                INSERT {
                    ?kgGraph dqv:hasQualityMeasurement [
                        a dqv:QualityMeasurement ;
                        rdfs:label "Unique IDs"@en ;
                        dqv:isMeasurementOf <https://w3id.org/fair/principles/latest/F1#F1A> ;
                        dqv:value 1
                    ] .
                } WHERE {
                    GRAPH ?kgGraph {
                        ?kg a dcat:Dataset .
                        ?s ?p ?o .
                    }
                    FILTER(isBlank(?s) || isBlank(?p) || isBlank(?o))
                }
                """
        ]
    ) .

<F1B.ttl> a mf:ManifestEntry ;
    dcterms:description "Weak evaluation should check the dataset identifier namespace in identifiers.org. However, has identifier?org has no SPARQL endpoint, we cannot check this." ;
    kgi:onSuccess (
        [
            dcterms:description "There is no dataset identifier." ;
            mf:action """
                PREFIX void: <http://rdfs.org/ns/void#>
                PREFIX dcat: <http://www.w3.org/ns/dcat#>
                PREFIX dcmitype: <http://purl.org/dc/dcmitype/>
                PREFIX schema: <http://schema.org/>
                PREFIX sd: <http://www.w3.org/ns/sparql-service-description#>
                PREFIX prov: <http://www.w3.org/ns/prov#>
                PREFIX pav: <http://purl.org/pav/>
                PREFIX foaf: <http://xmlns.com/foaf/0.1/>
                PREFIX dqv: <http://www.w3.org/ns/dqv#>
                PREFIX dce: <http://purl.org/dc/elements/1.1/>
                PREFIX dcterms: <http://purl.org/dc/terms/>
                PREFIX dataid: <http://dataid.dbpedia.org/ns/core#>
                INSERT {
                    ?kgGraph dqv:hasQualityMeasurement [
                        a dqv:QualityMeasurement ;
                        rdfs:label "Persistent IDs"@en ;
                        dqv:isMeasurementOf <https://w3id.org/fair/principles/latest/F1#F1B> ;
                        dqv:value 0
                    ] .
                } WHERE {
                    GRAPH ?kgGraph {
                        ?kg a dcat:Dataset .
                        FILTER( NOT EXISTS {
                            {
                                ?kg schema:identifier ?id .
                            }
                            UNION {
                                ?kg dcterms:identifier ?id .
                            }
                        })
                    }
                }
                """
        ]
        [
            dcterms:description "There is a schema of dcterms identifier." ;
            mf:action """
                PREFIX void: <http://rdfs.org/ns/void#>
                PREFIX dcat: <http://www.w3.org/ns/dcat#>
                PREFIX dcmitype: <http://purl.org/dc/dcmitype/>
                PREFIX schema: <http://schema.org/>
                PREFIX sd: <http://www.w3.org/ns/sparql-service-description#>
                PREFIX prov: <http://www.w3.org/ns/prov#>
                PREFIX pav: <http://purl.org/pav/>
                PREFIX foaf: <http://xmlns.com/foaf/0.1/>
                PREFIX dqv: <http://www.w3.org/ns/dqv#>
                PREFIX dce: <http://purl.org/dc/elements/1.1/>
                PREFIX dcterms: <http://purl.org/dc/terms/>
                PREFIX dataid: <http://dataid.dbpedia.org/ns/core#>
                INSERT {
                    ?kgGraph dqv:hasQualityMeasurement [
                        a dqv:QualityMeasurement ;
                        rdfs:label "Persistent IDs"@en ;
                        dqv:isMeasurementOf <https://w3id.org/fair/principles/latest/F1#F1B> ;
                        dqv:value 2
                    ] .
                } WHERE {
                    GRAPH ?kgGraph {
                        ?kg a dcat:Dataset .
                        {
                            ?kg schema:identifier ?id .
                        }
                        UNION {
                            ?kg dcterms:identifier ?id .
                        }
                    }
                }
                """
        ]
    ) .

<F2A.ttl> a mf:ManifestEntry ;
    kgi:onSuccess (
        [
            dcterms:description "at least one embedded RDF triple" ;
            mf:action """
                PREFIX void: <http://rdfs.org/ns/void#>
                PREFIX dcat: <http://www.w3.org/ns/dcat#>
                PREFIX dcmitype: <http://purl.org/dc/dcmitype/>
                PREFIX schema: <http://schema.org/>
                PREFIX sd: <http://www.w3.org/ns/sparql-service-description#>
                PREFIX prov: <http://www.w3.org/ns/prov#>
                PREFIX pav: <http://purl.org/pav/>
                PREFIX foaf: <http://xmlns.com/foaf/0.1/>
                PREFIX dqv: <http://www.w3.org/ns/dqv#>
                PREFIX dce: <http://purl.org/dc/elements/1.1/>
                PREFIX dcterms: <http://purl.org/dc/terms/>
                PREFIX dataid: <http://dataid.dbpedia.org/ns/core#>
                INSERT {
                    ?kgGraph dqv:hasQualityMeasurement [
                        a dqv:QualityMeasurement ;
                        rdfs:label "Structured metadata"@en ;
                        dqv:isMeasurementOf <https://w3id.org/fair/principles/latest/F2#F2A> ;
                        dqv:value 0
                    ] .
                } 
                WHERE {
                    FILTER (
                        NOT EXISTS {
                            GRAPH ?kgGraph {
                                ?kg a dcat:Dataset .
                            }
                        }
                    )
                }
                """
        ]
        [
            dcterms:description "at least one embedded RDF triple" ;
            mf:action """
                PREFIX void: <http://rdfs.org/ns/void#>
                PREFIX dcat: <http://www.w3.org/ns/dcat#>
                PREFIX dcmitype: <http://purl.org/dc/dcmitype/>
                PREFIX schema: <http://schema.org/>
                PREFIX sd: <http://www.w3.org/ns/sparql-service-description#>
                PREFIX prov: <http://www.w3.org/ns/prov#>
                PREFIX pav: <http://purl.org/pav/>
                PREFIX foaf: <http://xmlns.com/foaf/0.1/>
                PREFIX dqv: <http://www.w3.org/ns/dqv#>
                PREFIX dce: <http://purl.org/dc/elements/1.1/>
                PREFIX dcterms: <http://purl.org/dc/terms/>
                PREFIX dataid: <http://dataid.dbpedia.org/ns/core#>
                INSERT {
                    ?kgGraph dqv:hasQualityMeasurement [
                        a dqv:QualityMeasurement ;
                        rdfs:label "Structured metadata"@en ;
                        dqv:isMeasurementOf <https://w3id.org/fair/principles/latest/F2#F2A> ;
                        dqv:value 1
                    ] .
                } WHERE {
                    GRAPH ?kgGraph {
                        ?kg a dcat:Dataset .
                    }
                    FILTER (
                        NOT EXISTS {
                            GRAPH ?kgGraph {
                                ?kg a dcat:Dataset .
                                ?kg ?p ?o . 
                            }
                            VALUES ?p { 
                                dcterms:title
                                dcterms:description
                                dcat:accessURL
                                dcat:downloadURL
                                dcat:endpointDescription
                                dcat:endpointURL 
                            }
                        }
                    )
                }
                """
        ]
        [
            dcterms:description "At least one of the discoverability properties was found in metadata" ;
            mf:action """
                PREFIX void: <http://rdfs.org/ns/void#>
                PREFIX dcat: <http://www.w3.org/ns/dcat#>
                PREFIX dcmitype: <http://purl.org/dc/dcmitype/>
                PREFIX schema: <http://schema.org/>
                PREFIX sd: <http://www.w3.org/ns/sparql-service-description#>
                PREFIX prov: <http://www.w3.org/ns/prov#>
                PREFIX pav: <http://purl.org/pav/>
                PREFIX foaf: <http://xmlns.com/foaf/0.1/>
                PREFIX dqv: <http://www.w3.org/ns/dqv#>
                PREFIX dce: <http://purl.org/dc/elements/1.1/>
                PREFIX dcterms: <http://purl.org/dc/terms/>
                PREFIX dataid: <http://dataid.dbpedia.org/ns/core#>
                INSERT {
                    ?kgGraph dqv:hasQualityMeasurement [
                        a dqv:QualityMeasurement ;
                        rdfs:label "Structured metadata"@en ;
                        dqv:isMeasurementOf <https://w3id.org/fair/principles/latest/F2#F2A> ;
                        dqv:value 2
                    ] .
                } WHERE {
                    GRAPH ?kgGraph {
                        ?kg a dcat:Dataset .
                        ?kg ?p ?o . 
                    }
                    VALUES ?p { 
                        dcterms:title
                        dcterms:description
                        dcat:accessURL
                        dcat:downloadURL
                        dcat:endpointDescription
                        dcat:endpointURL 
                    }
                }
                """
        ]
    ) .
