@prefix mf: <http://www.w3.org/2001/sw/DataAccess/tests/test-manifest#> .
@prefix kgi: <http://ns.inria.fr/kg/index#> .

<> a mf:Manifest ;
    kgi:requiredAssets (
    ) ;
    mf:include (
        <classes/_manifest.ttl>
        <properties/_manifest.ttl>
    ) ;
    mf:entries (
        <loadLOV.ttl>
    ) .

<loadLOV.ttl> a mf:ManifestEntry ;
    kgi:onSuccess (
        [
            mf:action """
            LOAD <https://raw.githubusercontent.com/Wimmics/IndeGx/VocabularyExperiment/data/LOV_redux.classes.ttl> ;
            LOAD <https://raw.githubusercontent.com/Wimmics/IndeGx/VocabularyExperiment/data/LOV_redux.properties.ttl> 
            """
        ]
    ) .