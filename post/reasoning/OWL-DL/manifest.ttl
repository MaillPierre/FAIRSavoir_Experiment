<allDisjointClasses.ttl> a mf:ManifestEntry ;
    kgi:onSuccess (
        [
            mf:action """PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
                PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
                PREFIX owl: <http://www.w3.org/2002/07/owl#>
                INSERT {
                    ?member owl:disjointWith ?item .
                }
                WHERE {
                    ?restriction rdf:type owl:AllDisjointClasses ;
                        owl:members ?list .
                    ?list rdf:rest*/rdf:first ?member .
                    {
                        SELECT DISTINCT ?item ?restrict WHERE
                        {
                        ?restrict rdf:type owl:AllDisjointClasses ;
                            owl:members ?list .
                        ?list rdf:rest*/rdf:first ?item .
                        }
                    }
                    BIND(?restriction AS ?restrict)
                    FILTER(?member != ?item)
                }"""
        ]
        [
            mf:action """PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
                PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
                PREFIX owl: <http://www.w3.org/2002/07/owl#>
                INSERT {
                    GRAPH ?graph {
                        ?member owl:disjointWith ?item .
                    }
                }
                WHERE {
                    GRAPH ?graph {
                        ?restriction rdf:type owl:AllDisjointClasses ;
                            owl:members ?list .
                        ?list rdf:rest*/rdf:first ?member .
                        {
                            SELECT DISTINCT ?item ?restrict WHERE
                            {
                            ?restrict rdf:type owl:AllDisjointClasses ;
                                owl:members ?list .
                            ?list rdf:rest*/rdf:first ?item .
                            }
                        }
                    }
                    BIND(?restriction AS ?restrict)
                    FILTER(?member != ?item)
                }"""
        ]
    ) .

