<classInclusion.ttl> a mf:ManifestEntry ;
    kgi:onSuccess (
        [
            mf:action """PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
                INSERT {
                    ?resource rdfs:subClassOf ?superClass .
                }
                WHERE {
                    ?resource rdfs:subClassOf ?class .
                    ?class rdfs:subClassOf+ ?superClass .
                }"""
        ]
        [
            mf:action """PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
                INSERT {
                    GRAPH ?graph {
                        ?resource rdfs:subClassOf ?superClass .
                    }
                }
                WHERE {
                    GRAPH ?graph {
                        ?resource rdfs:subClassOf ?class .
                        ?class rdfs:subClassOf+ ?superClass .
                    }
                }"""
        ]
    ) .

