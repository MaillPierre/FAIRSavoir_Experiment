<objectPropertyChainInclusion.ttl> a mf:ManifestEntry ;
    kgi:onSuccess (
        [
            mf:action """PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
                PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
                PREFIX owl: <http://www.w3.org/2002/07/owl#>
                INSERT {
                    ?resource ?objectProperty ?o .
                }
                WHERE {
                    ?objectProperty rdf:type owl:ObjectProperty ;
                        owl:propertyChainAxiom ?list .
                    ?list rdf:first ?prop1 .
                    ?list rdf:rest/rdf:first ?prop2 .
                    ?resource ?prop1 [ ?prop2 ?o ] .
                }"""
        ]
        [
            mf:action """PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
                PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
                PREFIX owl: <http://www.w3.org/2002/07/owl#>
                INSERT {
                    GRAPH ?graph {
                        ?resource ?objectProperty ?o .
                    }
                }
                WHERE {
                    GRAPH ?graph {
                        ?objectProperty rdf:type owl:ObjectProperty ;
                            owl:propertyChainAxiom ?list .
                        ?list rdf:first ?prop1 .
                        ?list rdf:rest/rdf:first ?prop2 .
                        ?resource ?prop1 [ ?prop2 ?o ] .
                    }
                }"""
        ]
    ) .

<classEquivalence.ttl> a mf:ManifestEntry ;
    kgi:onSuccess (
        [
            mf:action """PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
                PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
                PREFIX owl: <http://www.w3.org/2002/07/owl#>
                INSERT {
                    ?resource rdf:type ?equivClass .
                }
                WHERE {
                    ?resource rdf:type ?superClass.
                    {?superClass owl:equivalentClass ?equivClass .}
                        UNION
                    {?equivClass owl:equivalentClass ?superClass .}
                }"""
        ]
        [
            mf:action """PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
                PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
                PREFIX owl: <http://www.w3.org/2002/07/owl#>
                INSERT {
                    GRAPH ?graph {
                        ?resource rdf:type ?equivClass .
                    }
                }
                WHERE {
                    GRAPH ?graph {
                        ?resource rdf:type ?superClass.
                        {?superClass owl:equivalentClass ?equivClass .}
                            UNION
                        {?equivClass owl:equivalentClass ?superClass .}
                    }
                }"""
        ]
    ) .

