@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix earl: <http://www.w3.org/ns/earl#> .
@prefix kgi: <http://ns.inria.fr/kg/index#> .
@prefix prov: <http://www.w3.org/ns/prov#> .

<> rdf:type kgi:TestQuery ;
    dcterms:isReferencedBy <manifest.ttl> ;
    dcterms:title "Functional Object Property" ;
    dcterms:description "Since functionalProperty is a functional object property, resource can only have one value for functionalProperty. Since resource functionalProperty both o1 and o2, we can infer that o1 and o2 must be the same individual." ;
    kgi:query """PREFIX void: <http://rdfs.org/ns/void#>
    PREFIX kgi: <http://ns.inria.fr/kg/index#>
    PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
    PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
    PREFIX owl: <http://www.w3.org/2002/07/owl#>
    ASK {
        {
            ?resource ?functionalProperty ?o1 ,
                ?o2 .
            ?functionalProperty rdf:type owl:ObjectProperty , 
                owl:FunctionalProperty .
        }
        UNION {
            GRAPH ?graph {
                ?resource ?functionalProperty ?o1 ,
                    ?o2 .
                ?functionalProperty rdf:type owl:ObjectProperty , 
                    owl:FunctionalProperty .
            }
        }
        FILTER (str(?o1) !=  str(?o2))
    }"""  .