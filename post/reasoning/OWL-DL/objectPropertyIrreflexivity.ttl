@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix earl: <http://www.w3.org/ns/earl#> .
@prefix kgi: <http://ns.inria.fr/kg/index#> .
@prefix prov: <http://www.w3.org/ns/prov#> .

<> rdf:type kgi:TestQuery ;
    dcterms:isReferencedBy <manifest.ttl> ;
    dcterms:title "Object Property Irreflexivity" ;
    dcterms:description "Since resource has an irreflexiveProperty assertion pointing to itself, and irreflexiveProperty is a irreflexive property, we can infer that there is an inconsistency associated with resource." ;
    kgi:query """PREFIX void: <http://rdfs.org/ns/void#>
    PREFIX kgi: <http://ns.inria.fr/kg/index#>
    PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
    PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
    PREFIX owl: <http://www.w3.org/2002/07/owl#>
    ASK {
        {
            ?irreflexiveProperty rdf:type owl:IrreflexiveProperty .
            ?resource ?irreflexiveProperty ?resource .
        }
        UNION {
            GRAPH ?graph {
                ?irreflexiveProperty rdf:type owl:IrreflexiveProperty .
                ?resource ?irreflexiveProperty ?resource .
            }
        }
    }"""  .