@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix earl: <http://www.w3.org/ns/earl#> .
@prefix kgi: <http://ns.inria.fr/kg/index#> .
@prefix prov: <http://www.w3.org/ns/prov#> .

<> rdf:type kgi:TestQuery ;
    dcterms:isReferencedBy <manifest.ttl> ;
    dcterms:title "Object Union Of" ;
    dcterms:description "Since the class resource has a subclass or equivalent class relation with a class that comprises the union of list, which contains member member, we can infer that member is a subclass of resource." ;
    kgi:query """PREFIX void: <http://rdfs.org/ns/void#>
    PREFIX kgi: <http://ns.inria.fr/kg/index#>
    PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
    PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
    PREFIX owl: <http://www.w3.org/2002/07/owl#>
    ASK {
        {
            ?resource rdf:type owl:Class ;
                rdfs:subClassOf|owl:equivalentClass
                [ rdf:type owl:Class ;
                    owl:unionOf ?list ] .
            ?list rdf:rest*/rdf:first ?member .
        }
        UNION {
            GRAPH ?graph {
                ?resource rdf:type owl:Class ;
                    rdfs:subClassOf|owl:equivalentClass
                    [ rdf:type owl:Class ;
                        owl:unionOf ?list ] .
                ?list rdf:rest*/rdf:first ?member .
            }
        }
    }"""  .